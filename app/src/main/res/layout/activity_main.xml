<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">

    <View
        android:id="@+id/mainpict"
        android:layout_width="416dp"
        android:layout_height="371dp"
        android:layout_marginBottom="360dp"
        android:background="@drawable/mainpict"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/jam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/takota"
        android:gravity="top"
        android:text="@string/jam"
        android:textAppearance="@style/jam"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.06"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/some_id"
        android:layout_width="29dp"
        android:layout_height="136dp"
        android:fontFamily="@font/staatliches"
        android:gravity="top"
        android:text="@string/some_id"
        android:textAppearance="@style/some_id"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/jam"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.057"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/enjoy_your"
        android:layout_width="376dp"
        android:layout_height="69dp"
        android:layout_marginTop="341dp"
        android:layout_marginBottom="68dp"
        android:fontFamily="@font/staatliches"
        android:gravity="center_horizontal|top"
        android:lineSpacingExtra="0sp"
        android:text="@string/enjoy_your"
        android:textAppearance="@style/enjoy_your"
        app:layout_constraintBottom_toTopOf="@+id/music_anywh"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/mainpict"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/jam2"
        android:layout_width="344dp"
        android:layout_height="63dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="28dp"
        android:fontFamily="@font/staatliches"
        android:gravity="center_horizontal|top"
        android:lineSpacingExtra="0sp"
        android:text="@string/jam2"
        android:textAppearance="@style/jam2"
        app:layout_constraintBottom_toTopOf="@+id/music_anywh"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.587"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mainpict"
        app:layout_constraintVertical_bias="0.687"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/music_anywh"
        android:layout_width="336dp"
        android:layout_height="33dp"
        android:layout_marginTop="478dp"
        android:layout_marginBottom="220dp"
        android:fontFamily="@font/staatliches"
        android:gravity="center_horizontal|top"
        android:lineSpacingExtra="0sp"
        android:text="@string/music_anywh"
        android:textAppearance="@style/music_anywh"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/loginButton"
        android:layout_width="157dp"
        android:layout_height="57dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="28dp"
        android:background="#00FFFFFF"
        android:onClick="loginButton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/music_anywh"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/component1"
        tools:ignore="MissingConstraints"
        tools:layout_conversion_absoluteHeight="55dp"
        tools:layout_conversion_absoluteWidth="136dp"
        tools:srcCompat="@drawable/component1" />

    <ImageButton
        android:id="@+id/signupButton"
        android:layout_width="161dp"
        android:layout_height="57dp"
        android:layout_marginTop="64dp"
        android:background="#00FFFFFF"
        android:onClick="signupbutton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.88"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/music_anywh"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/component2" />

</androidx.constraintlayout.widget.ConstraintLayout>