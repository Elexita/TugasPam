<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background1"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/back_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="#00FFFFFF"
        android:onClick="back_second_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/navigate_before_24px_outlined"
        tools:ignore="OnClick" />

    <TextView
        android:id="@+id/login"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="27dp"
        android:layout_marginTop="82dp"
        android:fontFamily="@font/staatliches"
        android:gravity="top"
        android:lineSpacingExtra="0sp"
        android:text="@string/login"
        android:textAppearance="@style/login"
        android:textColor="#000000"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="347dp"
        android:layout_height="63dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="252dp"
        android:autofillHints=""
        android:background="@drawable/layout_bg"
        android:ems="10"
        android:fontFamily="@font/staatliches"
        android:inputType="textPersonName"
        android:text="@string/email"
        android:textAppearance="@style/email"
        android:textColor="#000000"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="346dp"
        android:layout_height="62dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="318dp"
        android:background="@drawable/layout_bg"
        android:ems="10"
        android:fontFamily="@font/staatliches"
        android:inputType="textPersonName"
        android:text="@string/pass"
        android:textAppearance="@style/pass"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.034" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="166dp"
        android:layout_height="55dp"
        android:layout_marginStart="138dp"
        android:layout_marginTop="460dp"
        android:layout_marginEnd="121dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/component1"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/component1" />

    <Button
        android:id="@+id/button"
        android:layout_width="281dp"
        android:layout_height="45dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="527dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="161dp"
        android:background="#00FFFFFF"
        android:fontFamily="@font/staatliches"
        android:onClick="signup_Text"
        android:text="Doesn't have an account? Make One!"
        android:textColor="#000000"
        android:textSize="18sp"
        app:iconTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="#00FFFFFF"
        tools:ignore="MissingConstraints,OnClick" />


</androidx.constraintlayout.widget.ConstraintLayout>